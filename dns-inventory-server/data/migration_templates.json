  {
    "id": "godaddy-domains-v1",
    "name": "GoDaddy Domains (Legacy)",
    "description": "Template for migrating old GoDaddy domain files (domains.json)",
    "type": "domains",
    "field_mappings": {
      "created_at": "discovery_date",
      "discovery_date": "discovery_date",
      "domain": "domain",
      "status": "status"
